function onhaki()
local args = {
    [1] = "On",
    [2] = game:GetService("Workspace").UserData["User_"..game.Players.LocalPlayer.UserId].Data.HakiLevel.Value
}

workspace.UserData["User_"..game.Players.LocalPlayer.UserId].III:FireServer(unpack(args))
end

function offhaki()
local args = {
    [1] = "Off",
    [2] = game:GetService("Workspace").UserData["User_"..game.Players.LocalPlayer.UserId].Data.HakiLevel.Value
}

workspace.UserData["User_"..game.Players.LocalPlayer.UserId].III:FireServer(unpack(args))
end

while wait() do
for i = 1,999 do
onhaki()
offhaki()
offhaki()
