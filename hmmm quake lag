for _, v in pairs(game:GetService("Players"):GetPlayers()) do
game.Workspace[tostring(v.Name)].Powers.Quake.CutEffects.ChildAdded:Connect(function(child)
task.wait()
child:Destroy()
end)
game.Workspace[tostring(v.Name)].Powers.Quake.GlowEffects.ChildAdded:Connect(function(child)
task.wait()
child:Destroy()
end)
game.Workspace[tostring(v.Name)].Powers.Quake.P4.ChildAdded:Connect(function(child)
task.wait()
child:Destroy()
end)
game.Workspace[tostring(v.Name)].Powers.Quake.Rocks.ChildAdded:Connect(function(child)
task.wait()
child:Destroy()
end)
end

game:GetService("Players").PlayerAdded:Connect(function(v)
game.Workspace[tostring(v.Name)].Powers.Quake.CutEffects.ChildAdded:Connect(function(child)
task.wait()
child:Destroy()
end)
game.Workspace[tostring(v.Name)].Powers.Quake.GlowEffects.ChildAdded:Connect(function(child)
task.wait()
child:Destroy()
end)
game.Workspace[tostring(v.Name)].Powers.Quake.P4.ChildAdded:Connect(function(child)
task.wait()
child:Destroy()
end)
game.Workspace[tostring(v.Name)].Powers.Quake.Rocks.ChildAdded:Connect(function(child)
task.wait()
child:Destroy()
end)
end)
