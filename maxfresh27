function amongus()
    for i, v in pairs(game:GetService("Workspace").Barrels:GetDescendants()) do
        if v:IsA("ClickDetector") then
            fireclickdetector(v)
        end
    end
    for i, v in pairs(game:GetService("Workspace").Island8.Kitchen:GetDescendants()) do
        if v:IsA("ClickDetector") then
            fireclickdetector(v)
        end
    end
end

