                    table.insert(Values2, Valuer2)
                end
                for _, m in pairs(Values2) do
                    if string.find(m, "Defense") then
                        _G.keepdef2 = true
                    end
                    if string.find(m, "Melee") then
                        _G.keepmelee2 = true
                    end
                    if string.find(m, "Sniper") then
                        _G.keepsnip2 = true
