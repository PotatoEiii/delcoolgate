                                    ):WaitForChild("III"):FireServer(unpack(args))

                                    local args = {
                                        [1] = "Off",
                                        [2] = haki
                                    }

                                    workspace:WaitForChild("UserData"):WaitForChild(
                                        "User_" .. game.Players.LocalPlayer.UserId
                                    ):WaitForChild("III"):FireServer(unpack(args))

                                    local args = {
                                        [1] = "On",
                                        [2] = haki
                                    }

                                    workspace:WaitForChild("UserData"):WaitForChild(
                                        "User_" .. game.Players.LocalPlayer.UserId
                                    ):WaitForChild("III"):FireServer(unpack(args))

                                    local args = {
                                        [1] = "Off",
                                        [2] = haki
                                    }

                                    workspace:WaitForChild("UserData"):WaitForChild(
                                        "User_" .. game.Players.LocalPlayer.UserId
                                    ):WaitForChild("III"):FireServer(unpack(args))

                                    local args = {
                                        [1] = "Off",
                                        [2] = haki
                                    }

                                    workspace:WaitForChild("UserData"):WaitForChild(
                                        "User_" .. game.Players.LocalPlayer.UserId
                                    ):WaitForChild("III"):FireServer(unpack(args))
                                end
                                wait(7)
                            end
                        )
                    end
                end
            )
        end
    end
)

