                task.wait()
                for Valuer, State in next, Valuer do
                    table.insert(Values, Valuer)
                end
                for _, n in pairs(Values) do
                    if string.find(n, "Defense") then
                        _G.keepdef = true
                    end
                    if string.find(n, "Melee") then
                        _G.keepmelee = true
                    end
                    if string.find(n, "Sniper") then
                        _G.keepsnip = true
                    end
                    if string.find(n, "Sword") then
                        _G.keepsword = true
                    end
                end
            end
        )

