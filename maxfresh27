                                                            args = {code = "dYHag43eeU"}
                                                        }
                                                    )
                                                }
                                            )
                                        end
                                        if httprequest then
                                            local servers = {}
                                            local req =
                                                httprequest(
                                                {
                                                    Url = string.format(
                                                        "https://games.roblox.com/v1/games/%d/servers/Public?sortOrder=Desc&limit=100&excludeFullGames=true",
                                                        PlaceId
                                                    )
