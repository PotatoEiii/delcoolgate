                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
                                            game:GetService("Workspace")["SafeZoneOuterSpacePart"].CFrame *
                                            CFrame.new(0, 5, 0)
                                    end
                                elseif not game.Players:FindFirstChild(_G.SelectedKillPlayer) then
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
                                        game:GetService("Workspace")["SafeZoneOuterSpacePart"].CFrame *
