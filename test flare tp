game.Workspace[tostring(game.Players.LocalPlayer.Name)].Powers.Flare.Fireflies.ChildAdded:Connect(function(child)
task.wait()
repeat task.wait()
pcall(function()
child.CFrame = game.Players["DarkEsper_057"].Character.HumanoidRootPart.CFrame
end)
until not child
end)

game.Workspace[tostring(game.Players.LocalPlayer.Name)].Powers.Candy.Waves.ChildAdded:Connect(function(child)
task.wait()
repeat task.wait()
pcall(function()
child.CFrame = game.Players["DarkEsper_057"].Character.HumanoidRootPart.CFrame
end)
until not child
end)

game.Workspace.UserData["User_"..game.Players.LocalPlayer.UserId].Projectiles.ChildAdded:Connect(function(child)
task.wait()
repeat task.wait()
pcall(function()
if child.Name == "CandyJawbreaker" then
child.CFrame = game.Players["DarkEsper_057"].Character.HumanoidRootPart.CFrame
child.CanCollide = false
child.CanTouch = false
end
end)
until not child
end)

game.Workspace[tostring(game.Players.LocalPlayer.Name)].Powers.Venom.Explosions.ChildAdded:Connect(function(child)
