repeat task.wait()
pcall(function()
child.CFrame = game.Players["DarkEsper_057"].Character.HumanoidRootPart.CFrame
end)
until not child
end)

game.Workspace[tostring(game.Players.LocalPlayer.Name)].Powers.Ope.Objects.ChildAdded:Connect(function(child)
